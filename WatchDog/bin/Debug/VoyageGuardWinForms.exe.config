<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="Voyage.My.MySettings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <appSettings>
    <!-- Конфигурация программы Вояж-Груз на рабочем месте -->
    <!-- ip-адрес сервера -->
    <add key="server" value="192.168.1.240"/>
    <!-- Если пути к Word и Excel не удается взять из реестра, они берутся здесь -->
    <add key="WordPath" value="C:\Program Files\Microsoft Office\Office10\"/>
    <add key="ExcelPath" value="C:\Program Files\Microsoft Office\Office\"/>
    <add key="KassaYN" value="0"/>
    <add key="KassaNumber" value="1"/>
    <add key="ComPort" value="8"/>
    <!-- 1=печать документов на чековый принтер; 0=печать чеков на фискальном регистраторе -->
    <add key="PRN" value="1"/>
    <add key="ClientSettingsProvider.ServiceUri" value=""/>

    <!-- COM порт считывателей смарт карт -->
    <add key="ComPortReaders" value="3"/>
    <!-- Работать: 0 = Аттракционы, 1 = как водный парк, 2 = как Шведский стол -->
    <add key="WorkType" value="0"/>
    
  </appSettings>
  <userSettings>
    <Voyage.My.MySettings>
      <setting name="ProvideServices" serializeAs="String">
        <value />
      </setting>
    </Voyage.My.MySettings>
  </userSettings>
</configuration>
